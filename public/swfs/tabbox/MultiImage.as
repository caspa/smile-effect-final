import mx.utils.Delegate;class MultiImage {	public static var length:Number = 0;	var id:Number;	var container:MovieClip;	var panel:MovieClip;	var mc:MovieClip;	var mask:MovieClip;	var width:Number;	var height:Number;	var progress_count:Number;	var src:String;	var loader:MultiImageLoader;	public static var img:Array=new Array();	function MultiImage(loader:MultiImageLoader, container:MovieClip, src:String, x:Number, y:Number, width:Number, height:Number) {		this.set_id();		this.progress_count = 0;		this.container = container;		this.panel = this.container.createEmptyMovieClip("image_panel"+this.id, this.container.getNextHighestDepth());		this.mask = this.panel.createEmptyMovieClip("multi_image_mask_"+this.id, this.panel.getNextHighestDepth());		this.mc = this.mask.createEmptyMovieClip("multi_image_mc_"+this.id, this.mask.getNextHighestDepth());		this.mc._lockroot = true;		this.src = src;		this.width = width;		this.height = height;		this.loader = loader;		this.store();		this.loader.store(this.src,this.mc);	}	function store() {		MultiImage.img[this.mc._name] = this;	}	function progress() {		this.progress_count++;		trace("load progress: "+this.src+" ("+this.progress_count+")");	}	function set_id():Void {		this.id = MultiImage.length++;	}	//PANEL	function move(x:Number, y:Number):Void {		this.panel._x = x;		this.panel._y = y;	}	function hide() {		this.panel._alpha = 0;		this.panel._visible = false;	}	function show() {		this.panel._alpha=100;		this.panel._visible = true;	}	//IMAGE	function init(targetMC:MovieClip):Void {		//FX.fade_to(this.panel,100,50);	}	//ACTION	function trigger() {		this.mc.play();	}}