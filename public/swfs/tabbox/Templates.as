/*class: Templatesauthor: Jean-Christophe Littfasscopyright: 2008description: Registrierung der Template-Klassen*/class Templates {	public static var templates:Array = new Array();	public static var selected;	function Templates() {	}	public static function get(name:String) {		switch (name) {		case "flipbox" :			Templates.selected = new FlipBox(_root);			break;		default :			Templates.selected = new Template();		}		Templates.templates[Templates.templates.length] = Templates.selected;		return Templates.selected;	}}